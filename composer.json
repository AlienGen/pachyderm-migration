{
    "name": "aliengen/pachyderm-migration",
    "description": "A migration tool for Pachyderm SQL database",
    "type": "library",
    "require": {
        "php": ">=8.4",
        "aliengen/pachyderm": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Timoth√© Mermet-Buffet",
            "email": "mermetbt@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aliengen\\Pachyderm\\Migration\\": "src/Migration/"
        }
    },
    "bin": [
        "bin/pachyderm-migration"
    ],
    "scripts": {
        "migration:setup": [
            "Aliengen\\Pachyderm\\Migration\\Composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "installer-name": "pachyderm-migration"
    }
}